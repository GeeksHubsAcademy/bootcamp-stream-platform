{
  "name": "bootcamp-stream-platform",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:front": "cd frontend && npm start",
    "start:back": "cd backend && npm start",
    "dev:back": "cd backend && npm run dev",
    "start": "concurrently \"yarn run start:front\" \"npm run start:back\"",
    "dev": "concurrently \"yarn run start:front\" \"npm run dev:back\"",
    "postinstall": "npm run install:front && npm run install:back",
    "install:front": "cd frontend && yarn install",
    "install:back": "cd backend && npm install",
    "test:front": "cd frontend && npm test",
    "test:back": "cd backend && npm test",
    "test": "concurrently \"yarn run test:front\" \"npm run test:back\""
  },
  "author": "jupegarnica",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^4.1.0"
  },
  "devDependencies": {}
}
